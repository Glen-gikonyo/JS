<%- include("partials/header"); -%> 

<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="postTitle" style="width: 100%;" autocomplete="off">
    </div>
    <div class="form-group">
    <label>Post</label>
    <textarea class="form-control"  name="postBody" id="" cols="138%" rows="7%"></textarea>
    </div>
    <button class="btn btn-primary" type="submit" name="Btn">Publish</button> 
</form>



<%- include("partials/footer"); -%> 